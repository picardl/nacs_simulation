Cell[BoxData[
 RowBox[{"compute\[Beta]", ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "conj", ",", "lpr", ",", "\[Alpha]MF", ",", "Rx", ",", "Ry", ",", "Rz", 
      ",", "\[Alpha]LFc"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"lpr", "[", 
       RowBox[{"\[Eta]_", ",", "\[Theta]w_"}], "]"}], "=", 
      RowBox[{
       RowBox[{"Exp", "[", 
        RowBox[{"-", 
         FractionBox[
          RowBox[{"\[ImaginaryI]", " ", "\[Eta]"}], "2"]}], "]"}], 
       RowBox[{"(", GridBox[{
          {
           RowBox[{
            SuperscriptBox[
             RowBox[{"Cos", "[", "\[Theta]w", "]"}], "2"], "+", 
            RowBox[{
             RowBox[{"Exp", "[", 
              RowBox[{"\[ImaginaryI]", " ", "\[Eta]"}], "]"}], 
             SuperscriptBox[
              RowBox[{"Sin", "[", "\[Theta]w", "]"}], "2"]}]}], 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "-", 
              RowBox[{"Exp", "[", 
               RowBox[{"\[ImaginaryI]", " ", "\[Eta]"}], "]"}]}], ")"}], 
            RowBox[{"Cos", "[", "\[Theta]w", "]"}], 
            RowBox[{"Sin", "[", "\[Theta]w", "]"}]}], "0"},
          {
           RowBox[{
            RowBox[{"(", 
             RowBox[{"1", "-", 
              RowBox[{"Exp", "[", 
               RowBox[{"\[ImaginaryI]", " ", "\[Eta]"}], "]"}]}], ")"}], 
            RowBox[{"Cos", "[", "\[Theta]w", "]"}], 
            RowBox[{"Sin", "[", "\[Theta]w", "]"}]}], 
           RowBox[{
            SuperscriptBox[
             RowBox[{"Sin", "[", "\[Theta]w", "]"}], "2"], "+", 
            RowBox[{
             RowBox[{"Exp", "[", 
              RowBox[{"\[ImaginaryI]", " ", "\[Eta]"}], "]"}], 
             SuperscriptBox[
              RowBox[{"Cos", "[", "\[Theta]w", "]"}], "2"]}]}], "0"},
          {"0", "0", "1"}
         }], ")"}]}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
        RowBox[{"Linear", " ", "phase", " ", "retarder", " ", 
         RowBox[{"matrix", ".", " ", 
          RowBox[{"qwp", ":", " ", "\[Eta]"}]}]}], " ", "=", " ", 
        FractionBox["\[Pi]", "2"]}], ",", " ", 
       RowBox[{
        RowBox[{"hwp", ":", " ", "\[Eta]"}], " ", "=", " ", 
        FractionBox["\[Pi]", "4"]}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\[Epsilon]", "[", 
       RowBox[{"\[Eta]_", ",", "\[Theta]w_"}], "]"}], "=", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"lpr", "[", 
          RowBox[{"\[Eta]", ",", "\[Theta]w"}], "]"}], ".", 
         RowBox[{"(", GridBox[{
            {"1"},
            {"0"},
            {"0"}
           }], ")"}]}], ",", 
        RowBox[{"Assumptions", "->", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"0", "<=", "\[Eta]", "<", 
            RowBox[{"2", "\[Pi]"}]}], ",", 
           RowBox[{"0", "<=", "\[Theta]w", "<", 
            RowBox[{"2", "\[Pi]"}]}]}], "}"}]}]}], "]"}]}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"eRe", "[", 
       RowBox[{"t_", ",", "\[Eta]_", ",", "\[Theta]w_"}], "]"}], "=", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{
        RowBox[{"ComplexExpand", "[", 
         RowBox[{"Re", "[", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"\[Epsilon]", "[", 
             RowBox[{"\[Eta]", ",", "\[Theta]w"}], "]"}], 
            RowBox[{"Exp", "[", 
             RowBox[{
              RowBox[{"-", "\[ImaginaryI]"}], " ", "t"}], "]"}]}], ")"}], 
          "]"}], "]"}], ",", 
        RowBox[{"Assumptions", "->", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"0", "<=", "\[Theta]w", "<", 
            RowBox[{"2", "\[Pi]"}]}], ",", 
           RowBox[{"0", "<", "\[Eta]", "<", 
            RowBox[{"2", "\[Pi]"}]}], ",", 
           RowBox[{"0", "<=", "t", "<", 
            RowBox[{"2", "\[Pi]"}]}]}], "}"}]}]}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{"Jones", " ", 
       RowBox[{"matrices", ".", " ", "Polarization"}], " ", "of", " ", 
       "horizontal", " ", "linear", " ", "light", " ", "after", " ", "it", 
       " ", "passes", " ", "through", " ", "a", " ", "QWP", " ", "at", " ", 
       "angle", " ", "\[Theta]q", " ", "and", " ", "HWP", " ", "at", " ", 
       "angle", " ", "\[Theta]h"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"\[Alpha]MF", "=", 
      RowBox[{"(", GridBox[{
         {"\[Alpha]perp", "0", "0"},
         {"0", "\[Alpha]perp", "0"},
         {"0", "0", "\[Alpha]par"}
        }], ")"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "Polarizability", " ", "tensor", " ", "in", " ", "the", " ", 
       "molecular", " ", 
       RowBox[{"frame", ".", " ", "It"}], " ", "is", " ", "diagonal", " ", 
       "for", " ", "a", " ", "linear", " ", "molecule"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"\[Alpha]0", " ", "=", " ", 
      FractionBox[
       RowBox[{"\[Alpha]par", "+", 
        RowBox[{"2", "\[Alpha]perp"}]}], "3"]}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"Rx", "=", 
      RowBox[{"RotationMatrix", "[", 
       RowBox[{"\[Theta]", ",", 
        RowBox[{"{", 
         RowBox[{"1", ",", "0", ",", "0"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Ry", "=", 
      RowBox[{"RotationMatrix", "[", 
       RowBox[{"\[Theta]", ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", "1", ",", "0"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Rz", "=", 
      RowBox[{"RotationMatrix", "[", 
       RowBox[{"\[Phi]", ",", 
        RowBox[{"{", 
         RowBox[{"0", ",", "0", ",", "1"}], "}"}]}], "]"}]}], 
     RowBox[{"(*", 
      RowBox[{
      "Rotation", " ", "matrices", " ", "to", " ", "convert", " ", "from", 
       " ", "body", " ", "to", " ", "lab", " ", "frame"}], "*)"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"\[Alpha]LF", "=", 
      RowBox[{"FullSimplify", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Rz", "\[Transpose]"}], ".", 
         RowBox[{"Ry", "\[Transpose]"}], ".", "\[Alpha]MF", ".", "Ry", ".", 
         "Rz"}], ",", 
        RowBox[{"Assumptions", "->", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"\[Alpha]par", "\[Element]", "Reals"}], ",", 
           RowBox[{"\[Alpha]perp", "\[Element]", "Reals"}], ",", 
           RowBox[{"0", "<=", "\[Theta]", "<", 
            RowBox[{"2", "\[Pi]"}]}], ",", 
           RowBox[{"0", "<=", "\[Phi]", "<", 
            RowBox[{"2", "\[Pi]"}]}]}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Alpha]LFc", "=", 
      RowBox[{"\[Alpha]LF", "-", 
       RowBox[{"\[Alpha]0", " ", 
        RowBox[{"IdentityMatrix", "[", "3", "]"}]}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\[Beta]", "[", 
       RowBox[{"\[Theta]_", ",", "\[Phi]_"}], "]"}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"FullSimplify", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"\[Epsilon]", "[", 
             RowBox[{"\[Eta]", ",", "\[Theta]w"}], "]"}], 
            "\[ConjugateTranspose]"}], ".", 
           FractionBox[
            RowBox[{"(", 
             RowBox[{"-", "\[Alpha]LFc"}], ")"}], "\[Alpha]0"], ".", 
           RowBox[{"\[Epsilon]", "[", 
            RowBox[{"\[Eta]", ",", "\[Theta]w"}], "]"}]}], ",", 
          RowBox[{"Assumptions", "->", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"0", "<=", "\[Eta]", "<", 
              RowBox[{"2", "\[Pi]"}]}], ",", 
             RowBox[{"0", "<=", "\[Theta]w", "<", 
              RowBox[{"2", "\[Pi]"}]}], ",", 
             RowBox[{"0", "<=", "\[Theta]", "<", 
              RowBox[{"2", "\[Pi]"}]}], ",", 
             RowBox[{"0", "<=", "\[Phi]", "<", 
              RowBox[{"2", "\[Pi]"}]}], ",", 
             RowBox[{"\[Alpha]par", "\[Element]", "Reals"}], ",", 
             RowBox[{"\[Alpha]perp", "\[Element]", "Reals"}]}], "}"}]}]}], 
         "]"}], "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "1"}], "]"}], "]"}], "/.", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"\[Alpha]par", "-", "\[Alpha]perp"}], "->", 
          "\[CapitalDelta]"}], ",", 
         RowBox[{
          RowBox[{"\[Alpha]par", "+", 
           RowBox[{"2", "\[Alpha]perp"}]}], "->", 
          RowBox[{"3", "\[Alpha]00"}]}]}], "}"}]}]}], ";"}]}], 
   "]"}]}]], "Input",
