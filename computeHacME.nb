Cell[BoxData[
 RowBox[{
  RowBox[{"computeHacN", "[", "nMax_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"srAC", ",", "meAC"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"srAC", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"n1_", ",", "m1_"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"n2_", ",", "m2_"}], "}"}]}], "]"}], ":=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"n1", "==", "n2"}], "\[Or]", 
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{"n1", "-", "n2"}], "]"}], "==", "2"}]}], ")"}], "\[And]", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"m1", "==", "m2"}], "\[Or]", 
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{"m1", "-", "m2"}], "]"}], "==", "2"}]}], ")"}]}]}], 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "Selection", " ", "rules", " ", "for", " ", "coupling", " ", "between",
         " ", "rotational", " ", 
        RowBox[{"states", ".", " ", "These"}], " ", "rules", " ", "come", " ",
         "from", " ", "3", "J", " ", "symbols"}], ",", " ", 
       RowBox[{
       "see", " ", "Krems", " ", "or", " ", "Brown", " ", "and", " ", 
        RowBox[{"Carrington", "."}]}]}], "*)"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"meAC", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"Np_", ",", "Nmp_"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"N_", ",", "Nm_"}], "}"}]}], "]"}], ":=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"srAC", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"Np", ",", "Nmp"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"N", ",", "Nm"}], "}"}]}], "]"}], ",", 
        RowBox[{"Integrate", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"SphericalHarmonicY", "[", 
             RowBox[{"Np", ",", "Nmp", ",", "\[Theta]", ",", "\[Phi]"}], 
             "]"}], "\[Conjugate]"}], 
           RowBox[{"SphericalHarmonicY", "[", 
            RowBox[{"N", ",", "Nm", ",", "\[Theta]", ",", "\[Phi]"}], "]"}], 
           RowBox[{"\[Beta]", "[", 
            RowBox[{"\[Theta]", ",", "\[Phi]"}], "]"}], " ", 
           RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"\[Phi]", ",", "0", ",", 
            RowBox[{"2", "\[Pi]"}]}], "}"}]}], "]"}], ",", "0"}], "]"}]}], 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Matrix", " ", "element", " ", "from", " ", "AC"}], "-", 
       RowBox[{"electric", " ", "field"}]}], "*)"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"bN", "=", 
      RowBox[{"{", 
       RowBox[{"\"\<N\>\"", ",", "\"\<mN\>\""}], "}"}]}], 
     RowBox[{"(*", 
      RowBox[{"Rotational", " ", "quantum", " ", "numbers"}], "*)"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"lN", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"ToExpression", "[", "bN", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"N", ",", 
            RowBox[{"Range", "[", 
             RowBox[{"0", ",", "nMax"}], "]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"mN", ",", 
            RowBox[{"-", "N"}], ",", "N"}], "}"}]}], "]"}], ",", "1"}], 
       "]"}]}], 
     RowBox[{"(*", 
      RowBox[{
      "List", " ", "of", " ", "all", " ", "possible", " ", "rotational", " ", 
       "quantum", " ", "numbers"}], "*)"}], ";", "\[IndentingNewLine]", 
     RowBox[{"cN", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          SqrtBox[
           FractionBox[
            RowBox[{"4", "\[Pi]"}], 
            RowBox[{
             RowBox[{"2", 
              RowBox[{"#", "[", 
               RowBox[{"[", "1", "]"}], "]"}]}], "+", "1"}]]], 
          RowBox[{"SphericalHarmonicY", "[", 
           RowBox[{
            RowBox[{"#", "[", 
             RowBox[{"[", "1", "]"}], "]"}], ",", 
            RowBox[{"#", "[", 
             RowBox[{"[", "2", "]"}], "]"}], ",", "\[Theta]", ",", "\[Phi]"}],
            "]"}]}], ")"}], "&"}], "/@", "lN"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"HacN", "=", 
      RowBox[{"ParallelTable", "[", 
       RowBox[{
        RowBox[{"meAC", "[", 
         RowBox[{
          RowBox[{"lN", "[", 
           RowBox[{"[", "i", "]"}], "]"}], ",", 
          RowBox[{"lN", "[", 
           RowBox[{"[", "j", "]"}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"Length", "[", "lN", "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", 
          RowBox[{"Length", "[", "lN", "]"}]}], "}"}]}], "]"}]}], ";"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
