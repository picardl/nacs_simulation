Cell[BoxData[{
 RowBox[{
  RowBox[{"fillings", "=", 
   RowBox[{"ConstantArray", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"siteNumber", ",", 
       RowBox[{"Length", "[", "tRange", "]"}], ",", "nTrials"}], "}"}]}], 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"Do", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"siteFilling", "=", 
     RowBox[{"RandomVariate", "[", 
      RowBox[{
       RowBox[{"BernoulliDistribution", "[", "fillingFraction", "]"}], ",", 
       "siteNumber"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"While", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Total", "[", "siteFilling", "]"}], "==", "0"}], ",", 
      RowBox[{
       RowBox[{"siteFilling", "=", 
        RowBox[{"RandomVariate", "[", 
         RowBox[{
          RowBox[{"BernoulliDistribution", "[", "fillingFraction", "]"}], ",",
           "siteNumber"}], "]"}]}], ";", 
       RowBox[{"i", "++"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"sitePos", "=", 
     RowBox[{"Flatten", "[", 
      RowBox[{"Position", "[", 
       RowBox[{"siteFilling", ",", "1"}], "]"}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"nP", "=", 
     RowBox[{"Length", "[", "sitePos", "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"siteDistances", "=", 
     RowBox[{"Select", "[", 
      RowBox[{
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"i", ",", "j", ",", 
             RowBox[{"5", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"arrayPos", "[", 
                 RowBox[{"[", 
                  RowBox[{"sitePos", "[", 
                   RowBox[{"[", "j", "]"}], "]"}], "]"}], "]"}], "-", 
                RowBox[{"arrayPos", "[", 
                 RowBox[{"[", 
                  RowBox[{"sitePos", "[", 
                   RowBox[{"[", "i", "]"}], "]"}], "]"}], "]"}]}], ")"}]}]}], 
            "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", 
             RowBox[{"Range", "[", "nP", "]"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", 
             RowBox[{"Range", "[", "nP", "]"}]}], "}"}]}], "]"}], ",", "1"}], 
        "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"#", "[", 
          RowBox[{"[", "3", "]"}], "]"}], ">", "0"}], "&"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"nP", "<=", "1"}], ",", 
      RowBox[{
       RowBox[{"finalFilling", "=", 
        RowBox[{"siteFilling", "*", 
         RowBox[{"RandomVariate", "[", 
          RowBox[{
           RowBox[{"BernoulliDistribution", "[", "stirapFraction", "]"}], ",",
            "siteNumber"}], "]"}]}]}], ";"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"n\[ScriptCapitalH]n", "=", 
        SuperscriptBox["n\[ScriptCapitalH]1", "nP"]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"l\[ScriptCapitalH]n", "=", 
        RowBox[{"Tuples", "[", 
         RowBox[{"l\[ScriptCapitalH]1", ",", "nP"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"\[ScriptCapitalH]n", "=", 
        RowBox[{"IdentityMatrix", "[", "n\[ScriptCapitalH]n", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"s\[ScriptCapitalH]n", "=", 
        RowBox[{
         RowBox[{"Partition", "[", 
          RowBox[{"\[ScriptCapitalH]n", ",", 
           RowBox[{"{", 
            RowBox[{"n\[ScriptCapitalH]n", ",", "1"}], "}"}]}], "]"}], "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"s2l\[ScriptCapitalH]n", "=", 
        RowBox[{"AssociationThread", "[", 
         RowBox[{"s\[ScriptCapitalH]n", "->", "l\[ScriptCapitalH]n"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"l2s\[ScriptCapitalH]n", "=", 
        RowBox[{"AssociationThread", "[", 
         RowBox[{"l\[ScriptCapitalH]n", "->", "s\[ScriptCapitalH]n"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"idArray", "=", 
        RowBox[{"ConstantArray", "[", 
         RowBox[{"id", ",", "nP"}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"\[Sigma]x\[ScriptCapitalH]n", "=", 
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{"KroneckerProduct", "@@", 
           RowBox[{"ReplacePart", "[", 
            RowBox[{"idArray", ",", 
             RowBox[{"p", "->", 
              FractionBox["\[Sigma]x", "2"]}]}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"p", ",", "1", ",", "nP"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"num\[ScriptCapitalH]n", "[", "p_", "]"}], ":=", 
        RowBox[{"KroneckerProduct", "@@", 
         RowBox[{"ReplacePart", "[", 
          RowBox[{"idArray", ",", 
           RowBox[{"p", "->", 
            RowBox[{"\[Sigma]p", ".", "\[Sigma]m"}]}]}], "]"}]}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"\[Sigma]n\[ScriptCapitalH]n", "=", 
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{"KroneckerProduct", "@@", 
           RowBox[{"ReplacePart", "[", 
            RowBox[{"idArray", ",", 
             RowBox[{"p", "->", 
              FractionBox["\[Sigma]n", "2"]}]}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"p", ",", "1", ",", "nP"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"numAvg\[ScriptCapitalH]n", "=", 
        RowBox[{"Sum", "[", 
         RowBox[{
          FractionBox[
           RowBox[{"num\[ScriptCapitalH]n", "[", "p", "]"}], "nP"], ",", 
          RowBox[{"{", 
           RowBox[{"p", ",", "1", ",", "nP"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"id\[ScriptCapitalH]n", "=", 
        RowBox[{"KroneckerProduct", "@@", "idArray"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"\[Psi]0", "=", 
        RowBox[{"KroneckerProduct", "@@", 
         RowBox[{"ConstantArray", "[", 
          RowBox[{
           RowBox[{"l2s\[ScriptCapitalH]1", "[", "0", "]"}], ",", "nP"}], 
          "]"}]}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"op1", "=", 
        RowBox[{"N", "[", "\[Sigma]x\[ScriptCapitalH]n", "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"op2", "=", 
        RowBox[{"N", "[", 
         RowBox[{"id\[ScriptCapitalH]n", "+", 
          RowBox[{"Total", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"dd\[ScriptCapitalH]n", "[", "#", "]"}], "&"}], "/@", 
            "siteDistances"}], "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"\[Psi]", "=", 
        RowBox[{
         RowBox[{"MatrixExp", "[", 
          RowBox[{"\[ImaginaryI]", " ", "op1", " ", 
           FractionBox["\[Pi]", "2"]}], "]"}], ".", 
         RowBox[{"MatrixExp", "[", 
          RowBox[{
           RowBox[{"-", "\[ImaginaryI]"}], " ", "op2", " ", 
           RowBox[{"tRange", "[", 
            RowBox[{"[", "nt", "]"}], "]"}]}], "]"}], ".", 
         RowBox[{"MatrixExp", "[", 
          RowBox[{
           RowBox[{"-", "\[ImaginaryI]"}], " ", "op1", " ", 
           FractionBox["\[Pi]", "2"]}], "]"}], ".", "\[Psi]0"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"nBySite", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Re", "[", 
            RowBox[{"N", "[", 
             RowBox[{
              RowBox[{"\[Psi]", "\[ConjugateTranspose]"}], ".", 
              RowBox[{"num\[ScriptCapitalH]n", "[", "p", "]"}], ".", 
              "\[Psi]"}], "]"}], "]"}], "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "1"}], "]"}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"p", ",", "1", ",", "nP"}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"finalFilling", "=", 
        RowBox[{"imageEff", "*", 
         RowBox[{"ReplacePart", "[", 
          RowBox[{"siteFilling", ",", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"sitePos", "[", 
               RowBox[{"[", "p", "]"}], "]"}], "->", 
              RowBox[{"RandomVariate", "[", 
               RowBox[{"BernoulliDistribution", "[", 
                RowBox[{"nBySite", "[", 
                 RowBox[{"[", "p", "]"}], "]"}], "]"}], "]"}]}], ",", 
             RowBox[{"{", 
              RowBox[{"p", ",", "1", ",", "nP"}], "}"}]}], "]"}]}], "]"}]}]}],
        ";"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"fillings", "[", 
      RowBox[{"[", 
       RowBox[{"All", ",", "nt", ",", "trial"}], "]"}], "]"}], "=", 
