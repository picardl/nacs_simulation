Cell["\<\
The AC Stark shift only couples states whose N and mN differ by 0 or 2. Note \
that below I am only letting interactions occur between same N states, which \
is an approximation. The dipole dipole interaction only couples states that \
are exchanging states that are one N apart. Defining selection rules greatly \
speeds up the computation of the AC and DD Hamiltonians.\
\>", "Text",
 CellChangeTimes->{{3.884435566832438*^9, 3.884435602732943*^9}, {
  3.8844366278882093`*^9, 3.8844367199124413`*^9}, {3.884441577702096*^9, 
  3.88444159681872*^9}},ExpressionUUID->"1043ba2c-f497-42fc-8d56-\
745d486743b0"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"srAC", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"n1_", ",", "m1_"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"n2_", ",", "m2_"}], "}"}]}], "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"n1", "==", "n2"}], ")"}], "\[And]", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"m1", "==", "m2"}], "\[Or]", 
     RowBox[{
      RowBox[{"Abs", "[", 
       RowBox[{"m1", "-", "m2"}], "]"}], "==", "2"}]}], 
    ")"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"srDD", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"bn1_", ",", "bm1_"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"bn2_", ",", "bm2_"}], "}"}]}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"kn1_", ",", "km1_"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"kn2_", ",", "km2_"}], "}"}]}], "}"}]}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{"{", 
     RowBox[{"bn1", ",", "bm1"}], "}"}], "==", 
    RowBox[{"{", 
     RowBox[{"kn2", ",", "km2"}], "}"}]}], "\[And]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"bn2", ",", "bm2"}], "}"}], "==", 
    RowBox[{"{", 
     RowBox[{"kn1", ",", "km1"}], "}"}]}], "\[And]", 
   RowBox[{
    RowBox[{"Abs", "[", 
     RowBox[{"bn1", "-", "bn2"}], "]"}], "==", "1"}]}]}]}], "Input",
 CellChangeTimes->{{3.884435462149561*^9, 3.8844354696482964`*^9}, 
