Cell["\<\
The transition dipole moment between two states differing by angular momentum \
q  is given by dpME, taken from Wall et. al. The dipole dipole Hamiltonian \
induces correlated transitions between two molecules. These correlated \
transitions can be seen by the dipole-dipole matrix elements.\
\>", "Text",
 CellChangeTimes->{{3.8844364289224157`*^9, 3.884436470357112*^9}, {
  3.8844365114194717`*^9, 
  3.884436583053491*^9}},ExpressionUUID->"ee677288-0f62-454d-bdda-\
161010e7ed75"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"dpME", "[", 
    RowBox[{"q_", ",", 
     RowBox[{"{", 
      RowBox[{"Nb_", ",", "mb_"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"Nk_", ",", "mk_"}], "}"}]}], "]"}], ":=", 
   RowBox[{"Quiet", "[", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "mb"], " ", 
     SqrtBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "Nb"}], "+", "1"}], ")"}], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "Nk"}], "+", "1"}], ")"}]}]], " ", 
     RowBox[{"ThreeJSymbol", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"Nb", ",", 
         RowBox[{"-", "mb"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"1", ",", "q"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"Nk", ",", "mk"}], "}"}]}], "]"}], " ", 
     RowBox[{"ThreeJSymbol", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"Nb", ",", "0"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"1", ",", "0"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"Nk", ",", "0"}], "}"}]}], "]"}]}], "]"}]}], 
  RowBox[{"(*", 
   RowBox[{
   "dipole", " ", "matrix", " ", "element", " ", "for", " ", "spherical", " ",
     "component", " ", "q", " ", "between", " ", "bra", " ", "Nm", " ", "and",
     " ", "ket", " ", "Nm"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"HddME", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"b1_", ",", "b2_"}], "}"}], ",", 
    RowBox[{"{", 
     RowBox[{"k1_", ",", "k2_"}], "}"}]}], "]"}], ":=", 
