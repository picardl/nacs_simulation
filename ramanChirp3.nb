Cell[BoxData[
 RowBox[{
  RowBox[{"findACShift", "[", 
   RowBox[{"\[CapitalOmega]s0_", ",", "\[CapitalOmega]p0_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\[CapitalOmega]s", "=", "\[CapitalOmega]s0"}], ",", 
      RowBox[{"\[CapitalOmega]p", "=", "\[CapitalOmega]p0"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"P", "[", "t_", "]"}], "=", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"p1", "[", "t", "]"}], ",", 
        RowBox[{"p2", "[", "t", "]"}], ",", 
        RowBox[{"p3", "[", "t", "]"}], ",", 
        RowBox[{"p4", "[", "t", "]"}]}], "}"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "The", " ", "population", " ", "of", " ", "the", " ", "four", " ", 
       "states"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"init", "=", 
      RowBox[{
       RowBox[{"P", "[", "0", "]"}], "\[Equal]", 
       RowBox[{"{", 
        RowBox[{"1", ",", "0", ",", "0", ",", "0"}], "}"}]}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "The", " ", "population", " ", "is", " ", "initially", " ", "in", " ", 
       "the", " ", "initial", " ", "state"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"SE", "=", 
      RowBox[{
       RowBox[{"\[ImaginaryI]", " ", 
        RowBox[{
         SuperscriptBox["P", "\[Prime]",
          MultilineFunction->None], "[", "t", "]"}]}], "\[Equal]", 
       RowBox[{
        RowBox[{"H", "[", 
         RowBox[{
         "t", ",", "\[CapitalOmega]s", ",", "\[CapitalOmega]p", ",", 
          "\[CapitalDelta]0", ",", "0"}], "]"}], ".", 
        RowBox[{"P", "[", "t", "]"}]}]}]}], ";", 
     RowBox[{"(*", 
      RowBox[{"The", " ", "Schodinger", " ", "equation"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"sols", "=", 
      RowBox[{"ParametricNDSolveValue", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"SE", ",", "init"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"p1", ",", "p2", ",", "p3", ",", "p4"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", 
          RowBox[{"1", " ", "\[Mu]s"}]}], "}"}], ",", 
        RowBox[{"{", "\[CapitalDelta]0", "}"}], ",", 
        RowBox[{"PrecisionGoal", "\[Rule]", "7"}]}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"A", " ", "parametric", " ", "NDsolver"}], ",", " ", 
       RowBox[{
        RowBox[{"that", " ", "gives", " ", "a", " ", "time"}], "-", 
        RowBox[{
        "dependent", " ", "solution", " ", "as", " ", "a", " ", "function", 
         " ", "of", " ", "\[CapitalDelta]0"}]}]}], "*)"}], "\n", 
     RowBox[{"range\[CapitalDelta]", "=", 
      RowBox[{"{", 
       RowBox[{"0", ",", "5", ",", "1.5"}], "}"}]}], ";", " ", 
     RowBox[{"(*", 
      RowBox[{
      "The", " ", "population", " ", "in", " ", "the", " ", "initial", " ", 
       "state", " ", "is", " ", "checked", " ", "over", " ", "a", " ", 
       "range", " ", "of", " ", 
       RowBox[{"\[CapitalDelta]0s", ".", " ", "This"}], " ", "defines", " ", 
       "that", " ", 
       RowBox[{"range", "."}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"n", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"n", "<", "9"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"pop\[CapitalDelta]", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"Abs", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"sols", "[", 
                RowBox[{"\[CapitalDelta]", " ", "MHz"}], "]"}], 
               "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], "[", 
              RowBox[{".1", "\[Mu]s"}], "]"}], "]"}], "2"], ",", 
           RowBox[{"Evaluate", "@", 
            RowBox[{"Flatten", "[", 
             RowBox[{"{", 
              RowBox[{"\[CapitalDelta]", ",", "range\[CapitalDelta]"}], "}"}],
              "]"}]}]}], "]"}]}], 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{
          "A", " ", "table", " ", "of", " ", "the", " ", "population", " ", 
           "in", " ", "the", " ", "initial", " ", "state", " ", "at", " ", 
           "0.1", "\[Mu]s"}], ",", " ", 
          RowBox[{
           RowBox[{"depending", " ", "on", " ", 
            RowBox[{"\[CapitalDelta]0", ".", " ", "We"}], " ", "choose", " ", 
            "a", " ", "time", " ", "that", " ", "is", " ", "much", " ", 
            "less", " ", "than", " ", "a", " ", "\[Pi]", " ", 
            RowBox[{"pulse", ".", " ", "The"}], " ", "\[CapitalDelta]0", " ", 
            "that", " ", "corresponds", " ", "with", " ", "the", " ", 
            "lowest", " ", "initial", " ", "population", " ", "is", " ", 
            "the", " ", "2"}], "-", 
